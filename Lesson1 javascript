        //Задание 1
        let Tc = prompt("Введите температуру в градусах Цельсию");
        let Tf = 0;
        Tf = (9 / 5) * Tc + 32;
        alert("Температура по Фаренгейту составляет " + Tf + " градусов");

        //Задание 2
        let name = "Василий";
