        alert(admin);

        //Задание 3
        let result1 = 10 + 10 + "10"; //сперва сложились 10 и 10, получили 20, затем произошло преобразование в строку иог 2010
        console.log(result1);

        let rezult2 = 10 + "10" + 10; // произошло 2 раз преобразование в строку т.к. один из эолементов был строчный, а операция "+"
        console.log(rezult2);

        let rezult3 = 10 + 10 + +"10"; // произошло сложение 10 и 10 = 20 и двойной знак ++ преобразовал строку в число послучилось 30
        console.log(rezult3);

        let rezult4 = 10 / -""; // 10 разделили на -0  получили - бесконечность
        console.log(rezult4);

        let rezult5 = 10 / +"2,5"; // в результате выполнения 2-ъ операций подряд / деления и + сложения переменная получила значение NaN
        console.log(rezult5);
        //; (обратите внимание на пробелы, пишите также)

